{
   "name": "typos",
   "version": "1.0.0",
   "description": "A simple CSS and JS library",
   "main": "dist/js/typos.js",
   "scripts": {
     "build:css": "sass src/scss/main.scss dist/css/typos.css --source-map --no-error-css",
     "build:css:min": "sass src/scss/main.scss dist/css/typos.min.css --style=compressed --source-map --no-error-css",
     "build:js": "webpack --mode development",
     "build:js:min": "webpack --mode production",
     "build": "npm run build:css && npm run build:css:min && npm run build:js && npm run build:js:min"
   },
   "dependencies": {
     "jquery": "^3.6.0",
     "popper.js": "^1.16.1"
   },
   "devDependencies": {
     "sass": "^1.35.1",
     "webpack": "^5.38.1",
     "webpack-cli": "^4.7.2"
   },
   "browserslist": [
     "last 2 versions",
     "not dead",
     "not < 0.2%",
     "Firefox ESR",
     "maintained node versions"
   ]
 }
 